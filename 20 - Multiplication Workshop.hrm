-- 20 - Multiplication Workshop
-- Size: 22/15
-- Speed: 133/109
-- For each TWO THINGS in the INBOX,
-- MULTIPLY them, and OUTBOX the result.
-- Don't account for negative numbers.

main:
INBOX
JUMPZ firstNumberIsZero
COPYTO 0

INBOX
JUMPZ secondNumberIsZero
COPYTO 1
COPYTO 2

continue:
COPYFROM 2
ADD 1
COPYTO 2
BUMPDN 0
JUMPZ done
JUMP continue

firstNumberIsZero:
OUTBOX
INBOX
JUMP main

secondNumberIsZero:
OUTBOX
JUMP main

done:
COPYFROM 2
SUB 1
OUTBOX
JUMP main